{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "checkJs": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "lib": [
      "ESNext",
      "DOM",
    ],
    "types": [
      "bun-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.js"
  ]
}